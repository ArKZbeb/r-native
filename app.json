{
  "expo": {
    "name": "nativequiz",
    "slug": "nativequiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "nativequiz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "web",
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application a besoin de votre permission pour utiliser la cam√©ra.",
        "NSLocationWhenInUseUsageDescription": "Cette application a besoin de votre permission pour utiliser les notifications."
      },
      "bundleIdentifier": "com.samba953.nativequiz"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.samba953.nativequiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "notificationsPermission": "Allow $(PRODUCT_NAME) to send you notifications"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e38f2b5b-19df-4b33-a54a-47eb82dd7af7"
      }
    }
  }
}
